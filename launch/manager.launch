<!-- -*- mode: XML -*- -->

<launch>
    <node name="dynamixel_manager_arm" pkg="dynamixel_controllers" type="controller_manager.py" required="true" output="screen">
        <rosparam>
            namespace: dxl_manager
            serial_ports:
                gummi_d:
                    <!-- Equivalent to /dev/ttyUSB1 can be find at /dev/serial/by-id -->
                    port_name: "/dev/serial/by-id/usb-FTDI_FT232R_USB_UART_A7005LW2-if00-port0"
                    baud_rate: 1000000
                    min_motor_id: 1
                    max_motor_id: 40
                    update_rate: 100
                gummi_ae:
                    <!-- Equivalent to /dev/ttyUSB0 can be find at /dev/serial/by-id -->
                    port_name: "/dev/serial/by-id/usb-FTDI_FT232R_USB_UART_AL02L1EI-if00-port0"
                    baud_rate: 1000000
                    min_motor_id: 1
                    max_motor_id: 40
                    update_rate: 100
                gummi_se:
                    <!-- Equivalent to /dev/ttyUSB0 can be find at /dev/serial/by-id -->
                    port_name: "/dev/serial/by-id/usb-FTDI_FT232R_USB_UART_A8005icW-if00-port0"
                    baud_rate: 1000000
                    min_motor_id: 1
                    max_motor_id: 40
                    update_rate: 100
        </rosparam>
    </node>
</launch>
